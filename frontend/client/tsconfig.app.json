{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/theme.json"],
    "exclude": ["src/**/__tests__/*"],
    "types": [ "node" ],
    "typeRoots": [ "../node_modules/@types" ],
    "compilerOptions": {
      "composite": true,
      "baseUrl": ".",
      "lib": ["es2022", "dom"],
      "paths": {
        "@/*": ["./src/*"]
      },
      "resolveJsonModule": true
    }
  }
  
  